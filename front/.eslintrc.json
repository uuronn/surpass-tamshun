{
  "extends": ["prettier", "next/core-web-vitals"],
  "rules": {
    "semi": [2, "never"],
    "no-unused-vars": "off",
    "no-console": "off",
    "no-sparse-arrays": "off",
    "no-useless-constructor": "off",
    "array-callback-return": 0,
    "func-call-spacing": [2, "never"],
    "react-hooks/exhaustive-deps": "off"
  }
}
